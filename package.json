    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-images": "^0.5.19",
    "react-static": "^7.0.9",
    "react-static-plugin-mdx": "^7.0.8",
    "react-static-plugin-sitemap": "^7.0.8",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "tachyons": "^4.11.1"
  },
  "resolutions": {
    "react-static/webpack-dev-server": "~3.1.10"
  },
  "devDependencies": {
