    "react-static-plugin-source-filesystem": "^7.0.8",
    "react-static-plugin-styled-components": "^7.0.8",
    "react-static-plugin-typescript": "^7.0.8",
    "serve": "11.0.0",
    "terser": "^3.17.0",
    "typescript": "^3.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
